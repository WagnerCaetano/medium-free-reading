{
  "name": "Read Medium Sites for Free",
  "description": "Read Medium stories for free if you reached to free reading limit.",
  "manifest_version": 2,
  "version": "1.2",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*.medium.com/*",
        "https://*.entrepreneurshandbook.co/*"
      ]
    }
  ],
  "icons": {
    "128": "icons/128.png",
    "64": "icons/64.png",
    "16": "icons/16.png"
  }
}
